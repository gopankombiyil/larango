{
  "name": "gopankombiyil/larango",
  "description": "Laravel bridge for the ArangoDB Multi-model database",
  "keywords": [
    "laravel",
    "arangodb",
    "multi-model",
    "document",
    "graph",
    "database",
    "aranguent"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Gopan Kombiyil",
      "email": "gopan@mizaronline.com.au"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "^7.0",
    "triagens/arangodb": "3.6.*",
    "laravel-freelancer-nl/aql-query-builder": "^0.3",
    "ext-json": "*"
  },
  "require-dev": {
    "orchestra/testbench": "5.*",
    "phpunit/phpunit": "^8.4|^9.0",
    "mockery/mockery": "^1.3.1"
  },
  "repositories": [ ],
  "autoload": {
    "psr-4": {
      "GopanKombiyil\\Larango\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "GopanKombiyil\\Larango\\AranguentServiceProvider"
      ]
    }
  }
}
